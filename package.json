{
  "name": "shubh-api-gateways",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3005",
    "restart": "kill -port 3005 && next dev",
    "build": "next build",
    "start": "next start -p 3005",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:load": "k6 run tests/load/api-load-test.js",
    "test:load:media": "k6 run tests/load/media-upload-test.js",
    "migrate": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "migrate:specific": "node scripts/migrate.js",
    "seed": "node scripts/simple-seed.js",
    "seed:ecommerce": "node scripts/simple-seed.js",
    "data:archive": "node scripts/data-management.js archive",
    "data:stats": "node scripts/data-management.js stats",
    "data:cleanup": "node scripts/data-management.js cleanupOutbox",
    "data:worker": "node scripts/data-management.js startWorker",
    "data:process": "node scripts/data-management.js processOutbox",
    "data:indexes": "node scripts/data-management.js createIndexes",
    "data:validate": "node scripts/data-management.js validate",
    "data:help": "node scripts/data-management.js help",
    "setup:s3": "node scripts/setup-s3.js",
    "setup:minio": "node scripts/setup-minio.js",
    "setup:scylladb": "node scripts/setup-scylladb.js",
    "start:scylladb": "./scripts/start-scylladb.sh",
    "test:aws": "node scripts/test-aws-connection.js",
    "test:minio": "node scripts/test-minio.js",
    "test:backup": "curl -X POST http://localhost:3005/api/backup-blog -H \"Authorization: Bearer $JWT_TOKEN\"",
    "export:cleanup": "node scripts/data-management.js cleanupExports",
    "export:status": "node scripts/data-management.js exportStatus",
    "media:processor": "node scripts/start-media-processor.js",
    "media:test": "vitest run tests/integration/media-processing-pipeline.test.js",
    "test:system": "node scripts/run-tests.js",
    "test:comprehensive": "node scripts/test-system-comprehensive.js",
    "test:cpp": "node scripts/test-cpp-integration.js",
    "test:demo": "node scripts/run-tests.js",
    "production:setup": "./scripts/setup-docker-services.sh",
    "production:start": "./scripts/production-start.sh",
    "production:test": "./production-readiness-test.sh",
    "cpp:build": "cd cpp-system && make simple"
  },

    "helmet": "^8.1.0",
    "image-size": "^2.0.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "minio": "^8.0.5",
    "multer": "^2.0.2",
    "next": "^15.2.4",
    "next-auth": "^4.24.11",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.94.0",
    "pg": "^8.16.3",
